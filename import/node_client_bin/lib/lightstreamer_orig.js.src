    /*public*/ messageDiscarded: function(listenerProg) {
      var listener = this.getListener(listenerProg);
      
      this.fireEvent("onDiscarded",listener,[this.getOriginalMessage(listenerProg)]);
      
      this.clean(listenerProg);

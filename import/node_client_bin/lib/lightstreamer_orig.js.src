          }
      }());
    },
    
    /*private*/ addUniqueRequestInternal: function(tKey,request) {
        /*
         * If a request with the same key is already present, substitute the request whit the new one.
         * Otherwise add the request.
         */
        if (this.keys[tKey]) {

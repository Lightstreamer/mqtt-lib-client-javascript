                          if (requestsLogger.isDebugLogEnabled()) {
                              requestsLogger.logErrorExc(new Error("backtrace"),
                                      "Duplicated message",
                                      "seq=", seq, "prog=", prog, "ptr=", request === currentReq);
                          }
                          requestsLogger.logError(LoggerManager.resolve(200), "seq=", seq, "prog=", prog);
                      }

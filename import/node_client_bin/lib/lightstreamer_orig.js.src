define('FlashBridgeCallbacks',["LoggerManager","Constants"],function(LoggerManager,Constants){

  var flashLogger = LoggerManager.getLoggerProxy(Constants.FLASH);
  
  var FlashBridgeCallbacks = function(bridge,subId) {
    this.bridge = bridge;
    this.subId = subId;
  };
  

  FlashBridgeCallbacks.prototype = {
      
    onItemUpdate: function(itemObj) {
      //rinuncio a addField, rinuncio anche a isSnapshot?
      var ffObj = {}; 
      ffObj.length = itemObj.getNumFields();
      for (var i = 1; i <= ffObj.length; i++) {
        ffObj[i + "_old"] = itemObj.getValue(i);
        if (itemObj.isValueChanged(i)) {
          ffObj[i] = itemObj.getValue(i);
        } else {
          ffObj[i] = Constants.UNCHANGED;
        }
   
        var fldName = itemObj.getFieldName(i);

        if (fldName != null) {
          ffObj[fldName+"_pos"] = i;
        }
    
      }
      
      var itemPos = itemObj.getItemPos();
      var itemName = itemObj.getItemName();
      
      try {
        this.bridge.flashObj.onItemUpdate(this.subId, itemPos, ffObj, itemName);
      } catch (_e) {
        flashLogger.logWarn(LoggerManager.resolve(418),_e);
      }
    },
  
    onItemLostUpdates: function(itemName, itemPos, lostUpdates) {
      try {
        this.bridge.flashObj.onLostUpdates(this.subId,itemPos,lostUpdates,itemName);  
      } catch(_e) {
        flashLogger.logWarn(LoggerManager.resolve(419),_e);
      }
    },
              
    onEndOfSnapshot: function() {
      try {
        this.bridge.flashObj.onEndOfSnapshot(this.subId);
      } catch(_e) {
        flashLogger.logWarn(LoggerManager.resolve(420),_e);
      }
    },
      
    onClearSnapshot: function() {
      try {
        this.bridge.flashObj.onClearSnapshot(this.subId);
      } catch(_e) {
        flashLogger.logWarn(LoggerManager.resolve(421),_e);
      }
    },
      
    onSubscription: function(){
      try {
        this.bridge.flashObj.onStart(this.subId);
      } catch (_e) {
        flashLogger.logWarn(LoggerManager.resolve(422),_e);
      }
    }
  };
  
  return FlashBridgeCallbacks;

});


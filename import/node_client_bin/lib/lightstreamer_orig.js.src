define('SlowingHandler',["LoggerManager","Global","Helpers","ASSERT","Constants"], 
    function(LoggerManager,Global,Helpers,ASSERT,Constants){
  
  // Coefficiente della media mobile usata per raccogliere i campionamenti dei ritardi.
  var momentum = 5/10;
  //Livello massimo del ritardo ammesso, oltre il quale scatta l'aggiustamento (millisecondi).
  var maxMean = 7000;
  var hugeDelay = 20000;
  
  var protocolLogger = LoggerManager.getLoggerProxy(Constants.PROTOCOL);
  var sessionLogger = LoggerManager.getLoggerProxy(Constants.SESSION);
  
  var SlowingHandler = function(policyBean,engId) {
    
    //Istante di ricezione del primo segnale di sincronismo dal server; fa da riferimento per i successivi controlli del ritardo rispetto al server.
    this.refTime = 0;
    // Ritardo medio rispetto al server finora misurato (millisecondi).
    this.meanElaborationDelay = null;
    
    this.hugeFlag = false;
    
    this.policyBean = policyBean;
    
    this.session = null;
  };
  
  
  SlowingHandler.prototype = {
    
    
    /*public*/ toString: function() {
      return ["[","SlowingHandler",this.meanElaborationDelay,this.refTime,momentum,maxMean,"]"].join("|");
    },
    
    /*public*/ isLate: function(lateVal) {
      return this.meanElaborationDelay!=null  && this.meanElaborationDelay > lateVal;
    },
    
    /*public*/ changeSession: function(newSession) {
      this.session = newSession;
    },
    
    /*public*/ getDelay: function() {
      /*if (!this.policyBean.slowingEnabled) {
        return null;
      }*/
      return this.meanElaborationDelay!=null && this.meanElaborationDelay>0 ? Math.round(this.meanElaborationDelay) : null;
    },
    
    /*public*/ resetSync: function() {
      this.meanElaborationDelay = null;
      this.hugeFlag = false;
    },
    
    /*public*/ prepareForSync: function() {
      this.hugeFlag = false;
    },
   
    /*public*/ testPollSync: function(millis) {
      // riusiamo il meccanismo di calcolo delle statistiche streaming
      this.testSync(millis);
      // ignoriamo il risultato; per il polling, se i ritardi sono troppo alti,
      // potremmo incappare in un sync error con successiva riconnessione
    },
    
    /*private*/ syncCheck: function(secs) {
          if (!ASSERT.verifyOk(this.session.isReceivingAnswer())) {
        protocolLogger.logError(LoggerManager.resolve(194));
      }
          // richiesta di controllo della sincronizzazione
      if (this.session.isStreamingSession()) {
        // in caso polling i sync non arrivano
        // e peraltro non sarebbero significativi
        var syncProblem = this.testSync(secs * 1000);

        if (!syncProblem) {
          this.session.onSynchOk();
          
        } else if (this.policyBean.slowingEnabled) {
          // potremmo usare i constrain per ridurre il flusso,
          // ma abbiamo visto che andare in polling e' sufficiente
          this.session.onSlowRequested();
          
          // il ritardo medio (meanElaborationDelay) del caso streaming
          // rimane come stima iniziale del ritardo previsto ad ogni poll
        }
    
      }
      
    },
    
    /*public*/ startSync: function(isPolling) {
      if (!isPolling) {
        // per convenzione, in streaming resettiamo le misurazioni
        // dei ritardi medi a ogni nuova sessione
        this.resetSync();
      } else {
        // manteniamo la stima gia' accumulata, non solo nei precedenti cicli,
        // ma anche nella precedente sessione, addirittura se era di streaming;
        // tanto la cosa ha il solo effetto di aumentare la persistenza della
        // sessione richiesta al Server; dobbiamo solo cercare di non esagerare
      }
      
      this.refTime = Helpers.getTimeStamp();
    },
    
    /*private*/ testSync: function(millis) {
      // controllo euristico per stabilire se il client rimane indietro
      // ed eventuale rilancio del server con modifica euristica del tempo minimo di refresh
      var currTime = Helpers.getTimeStamp();
      
      
      if (!this.refTime) {
        //this cannot happen..btw we handle the case signaling a delay problem
        return true;
      }
      
  /////// scommentare per far scattare l'euristica: //////////////////////////////////////////////////////////////////////////
      // this.refTime -= 2000;
      
      /*cc = !cc ? 1 : cc+1;
       if (cc == 4) {
         this.refTime -= 200000;
       } else if (cc == 5) {
         this.refTime += 200000;
       }*/
      
      var diffTime = (currTime - this.refTime) - millis;
      // faccio una media mobile dei ritardi;
      // non tengo conto della distanza tra le diverse osservazioni
      if (this.meanElaborationDelay == null) {
        this.meanElaborationDelay = diffTime;
        sessionLogger.logDebug(LoggerManager.resolve(197));
        return false;        // almeno la prima volta non controllo
        
      } else {
        
        //detect unlucky sleep
        //we should avoid false positive...how? 
        //If we got a huge delay we forgive the first one but not the second one 
        if (diffTime > hugeDelay && diffTime > this.meanElaborationDelay*2) {
          this.hugeFlag = !this.hugeFlag;
          if (this.hugeFlag) {
            sessionLogger.logInfo(LoggerManager.resolve(195));
            //let's try to ignore this check, the pc probably slept; we should get a sync error soon
            return this.meanElaborationDelay > maxMean;
          }
        } 
        
        
        this.meanElaborationDelay = this.meanElaborationDelay * momentum + diffTime * (1 - momentum);

        if (this.meanElaborationDelay < 60) { //the executer runs every 50ms, we can forgive such delay
          this.meanElaborationDelay = 0;
          sessionLogger.logDebug(LoggerManager.resolve(198));
          return false;
              
        } else if (this.isLate(maxMean)) {
          // sto andando particolarmente male
          sessionLogger.logInfo(LoggerManager.resolve(196));
          return true;
            
        } else {
          sessionLogger.logDebug(LoggerManager.resolve(199));
          return false;
            
        }
      }
    }
    

  };
  
  return SlowingHandler;
  
});

define('ControlRequestBatch',["LoggerManager","ControlRequest","ASSERT","Constants"],
    function(LoggerManager,ControlRequest,ASSERT,Constants) {
  
  var requestsLogger = LoggerManager.getLoggerProxy(Constants.REQUESTS);
  
  function ControlRequestBatch(_batchType) {

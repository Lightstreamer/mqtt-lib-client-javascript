define('Assertions',["ASSERT", "LoggerManager"], function(ASSERT,LoggerManager) {
    
    var log = LoggerManager.getLoggerProxy("assertions");
    
    /**
     * Assertion utilities.
     * 
     * @author Alessandro Carioni
     * @since May 2018
     */
    var Assertions = {
            assert: function(cond, msg) {
                if (! cond) {
                   this.fail(msg);
                }
            },
            
            assertEquals: function(a, b, msg) {
                if (a !== b) {
                    this.fail(msg + ": Expected " + a + " but found " + b);
                }
            },
            
            implies: function(a, b, msg) {
                this.assert(! a || b, msg);
            },
            
            fail: function(msg) {
                ASSERT.fail();
                if (msg != null) {
                    log.logError(msg);
                }
            },
            
            

            verifyOk: function(a, msg) {
                if (! ASSERT.verifyOk(a)) {
                    if (msg != null) {
                        log.logError(msg);
                    }
                }
            },
            
            verifyNotOk: function(a, msg) {
                if (! ASSERT.verifyNotOk(a)) {
                    if (msg != null) {
                        log.logError(msg);
                    }
                }
            },
            
            verifyValue: function(a, b, msg) {
                if (! ASSERT.verifyValue(a, b, null)) {
                    if (msg != null) {
                        log.logError(msg);
                    }
                }
            },
            
            verifyDiffValue: function(a, b, msg) {
                if (! ASSERT.verifyDiffValue(a, b, null)) {
                    if (msg != null) {
                        log.logError(msg);
                    }
                }
            }
    };
    
    return Assertions;
});

define('MessageProxy',["Executor","List","Assertions"],function(Executor,List,Assertions) {
  
  var MessageProxy = function(engineHandler) {
    this.lastMessageListener = -1;

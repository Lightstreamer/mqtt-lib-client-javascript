      },
      
      /*private*/ onEvent: function() {
        if (this.phase == Session.FIRST_BINDING) {
          resetMaybeOnline();
        }
        
        if (this.needsHourglassTrick() && (this.phase == Session.BINDING || this.phase == Session.FIRST_BINDING)) {
